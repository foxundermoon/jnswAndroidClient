<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FAB">
        <!-- Background color -->
        <attr name="bg_color" format="color|reference"/>
        <attr name="bg_color_pressed" format="color|reference"/>
    </declare-styleable>

    <declare-styleable name="ImageTextButton">
        <attr name="text" format="reference|string" />
        <attr name="image" format="reference" />
        <attr name="image_width" format="dimension" />
        <attr name="image_height" format="dimension"/>
        <attr name="image_min_width" format="dimension" />
        <attr name="image_min_height" format="dimension" />
        <attr name="image_max_width" format="dimension" />
        <attr name="image_max_height" format="dimension" />
        <attr name="text_size" format="dimension" />
        <attr name="text_color" format="reference|color" />
    </declare-styleable>


    <declare-styleable name="CloseableMenuGroupLayoutStyleable">
        <attr name="title_text" format="reference|string" />
        <attr name="title_color" format="reference|color" />
        <attr name="title_textSize" format="dimension" />
    </declare-styleable>
