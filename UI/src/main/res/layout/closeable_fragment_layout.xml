<?xml version="1.0" encoding="utf-8"?>
<com.jnsw.android.ui.widget.CloseableMenuGroupLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/Dark_Dark"
    android:orientation="horizontal"
    tools:context="com.jnsw.android.ui.widget.FloatCenterFragment"
    android:gravity="center">

    <HorizontalScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="35dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="40dp"
        android:layout_marginTop="35dp">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/closeable_menu_group_layout_linelayout_container"
            android:tag="@string/closeable_menu_group_layout_linelayout_container"
            android:orientation="horizontal">
        </LinearLayout>
    </HorizontalScrollView>
</com.jnsw.android.ui.widget.CloseableMenuGroupLayout>
